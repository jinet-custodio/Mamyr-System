Use mamyr

CREATE TABLE notification (
    notificationID INT AUTO_INCREMENT PRIMARY KEY,
    bookingID INT NULL,
    partnershipID INT NULL,
    userID INT NOT NULL,
    message TEXT NOT NULL,
    receiver ENUM ('Admin', 'Partner', 'Customer') NOT NULL,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    is_read BOOLEAN DEFAULT 0,

    FOREIGN KEY (bookingID) REFERENCES booking(bookingID),
    FOREIGN KEY (partnershipID) REFERENCES partnership(partnershipID),
    FOREIGN KEY (userID) REFERENCES user(userID)
    );

ALTER TABLE notification 
CHANGE userID senderID INT NOT NULL;
ALTER TABLE notification 
ADD receiverID INT NOT NULL 
AFTER senderID;

ALTER TABLE `notification` 
ADD CONSTRAINT `notification_ibfk_4` 
FOREIGN KEY (`receiverID`) REFERENCES `user`(`userID`) 
ON DELETE RESTRICT 
ON UPDATE RESTRICT;

CREATE TABLE userreview (
    userReviewID INT PRIMARY KEY AUTO_INCREMENT,
    bookingID INT,
    bookingType VARCHAR(50),
    reviewRating DECIMAL(2,1),
    reviewComment TEXT,
    FOREIGN KEY (bookingID) REFERENCES booking(bookingID)
);
